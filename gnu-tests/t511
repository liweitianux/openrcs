. $srcdir/common
split_std_out_err no
. $srcdir/common-i

##
# Like t510, for ‘ci -iREV -mm’.
##

$hey echo '                                      -*- org -*-'

wdisp=removed
rev_rx='^4\.20$'
log_rx='^@OK$'

triple "ci -i4.20 -mOK"
triple 'ci -i4.20 -mOK -w00t'
triple 'ci -i4.20 -mOK -nomoly'
triple 'ci -i4.20 -mOK -w00t -nomoly'

exit 0

